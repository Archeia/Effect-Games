	<Record Path="/admin/article_categories">
		<Data>
			<Category Path="/main" Title="Misc Articles"/>
			<Category Path="/news" Title="Effect News"/>
			<Category Path="/community" Title="Community Blog"/>
			<Category Path="/docs" Title="Documentation"/>
			<Category Path="/tutorials" Title="Tutorials"/>
			<Category Path="/plugins" Title="Plugins"/>
			<Category Path="/engine" Title="Dev Blog"/>
