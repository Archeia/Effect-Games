<?xml version="1.0"?>
<Setup>
	<Record Path="/admin/ticket_systems">
		<Data>
			<System Title="Bug Tracker" Path="/bugs">
				<Notify>you@yourdomain.com</Notify>
				<Icon>bug</Icon>
				<RecordTypeName>Bug</RecordTypeName>
				<Field ID="summary" Title="Summary" Type="Text" Default="" Size="50" MaxLength="128" Required="1"/>
				<Field ID="description" Title="Description" Type="TextArea" Default="" Cols="80" Rows="10" MaxLength="8192"/>
				<Field ID="assigned" Title="Assigned To" Type="Text" Default="" Special="Assigned" Size="20" MaxLength="32" Index="1"/>
				<Field ID="engine" Title="Engine" Type="Menu" Default="" Special="EngineVersions" Index="1"/>
				<Field ID="category" Title="Category" Type="Menu" Default="Bug" Items="Bug, Change Request, Feature Request" Index="1"/>
				<Field ID="status" Title="Status" Type="Menu" Default="New" Items="New, Active, Replied, Postponed, Cancelled, Closed" Index="1"/>
				<Field ID="priority" Title="Priority" Type="Menu" Default="None" Items="None, Low, Medium, High" Index="1"/>
				<Field ID="software" Title="Software" Type="Text" Default="" Size="20" Special="UserSoftware" Index="1"/>
				<Field ID="cc" Title="Cc" Type="Text" Default="" Size="30" MaxLength="1024" Special="Cc"/>
			</System>
			<System Title="Help Desk" Path="/helpdesk">
				<Notify>you@yourdomain.com</Notify>
				<Icon>telephone</Icon>
				<RecordTypeName>Ticket</RecordTypeName>
				<Field ID="summary" Title="Summary" Type="Text" Default="" Size="50" MaxLength="128" Required="1"/>
				<Field ID="description" Title="Description" Type="TextArea" Default="" Cols="80" Rows="10" MaxLength="8192"/>
				<Field ID="assigned" Title="Assigned To" Type="Text" Default="" Special="Assigned" Size="20" MaxLength="32" Index="1"/>
				<Field ID="category" Title="Category" Type="Menu" Default="General" Items="General, Question, Suggestion, JavaScript Error, Performance Issue, Other" Index="1"/>
				<Field ID="status" Title="Status" Type="Menu" Default="New" Items="New, Active, Replied, Postponed, Cancelled, Closed" Index="1"/>
				<Field ID="priority" Title="Priority" Type="Menu" Default="None" Items="None, Low, Medium, High" Index="1"/>
				<Field ID="game" Title="Game ID" Type="Text" Default="" Size="20" MaxLength="32" Index="1"/>
				<Field ID="cc" Title="Cc" Type="Text" Default="" Size="30" MaxLength="1024" Special="Cc"/>
			</System>
		</Data>
	</Record>
