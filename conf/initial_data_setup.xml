			<STag Name="sidebar_docs" Title="Documentation Sidebar"/>
			<STag Name="sidebar_tutorials" Title="Tutorials Sidebar"/>
			<STag Name="sidebar_plugins" Title="Plugins Sidebar"/>
			<STag Name="featured_game" Title="Featured Game"/>
